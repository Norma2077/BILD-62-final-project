    "import numpy as np\n",
    "def absorbancecurve(x):\n",
    "    mean=np.mean(x)\n",
    "    std=np.std(x)\n",
    "    y= 1/(std * np.sqrt(2 * np.pi)) * np.exp( - (x - mean)**2 / (2 * std**2))\n",
    "    return y\n"
   ]
